{
  "python-module": {
    "adapter": "debugpy",
    "filetypes": [
      "python"
    ],
    "breakpoints": {
      "exception": {
        "raised": "",
        "uncaught": "",
        "userUnhandled": ""
      }
    },
    "configuration": {
      "name": "python: Module",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "python": "~/miniconda3/bin/python3",
      "cwd": "${fileDirname}",
      "module": "",
      "args": []
    }
  }
}
