{
  "configurations": {
    "cpp:launch": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "name": "cpp",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}/${fileBasenameNoExtension}",
        "args": [
          "*${ProgramArgs}"
          ],
          "cwd": "${workspaceRoot}",
          "environment": [],
          "externalConsole": true,
          "stopOnEntry": false,
          "MIMode": "gdb",
          "logging": {
            "engineLogging": false
          }
      }
    }
  }
}
