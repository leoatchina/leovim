[
  // focusGroup
  {
    "key": "shift+alt+right",
    "command": "workbench.action.focusRightGroupWithoutWrap"
  },
  {
    "key": "shift+alt+left",
    "command": "workbench.action.focusLeftGroupWithoutWrap"
  },
  {
    "key": "shift+alt+up",
    "command": "workbench.action.focusAboveGroupWithoutWrap"
  },
  {
    "key": "shift+alt+down",
    "command": "workbench.action.focusBelowGroupWithoutWrap"
  },
  // outline focus
  {
    "key": "ctrlcmd+shift+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "ctrlcmd+shift+t",
    "command": "workbench.action.showAllSymbols"
  },
  {
    "key": "ctrlcmd+t",
    "command": "workbench.action.showAllSymbols"
  },
  {
    "key": "ctrlcmd+shift+,",
    "command": "-editor.action.inPlaceReplace.up"
  },
  {
    "key": "ctrlcmd+shift+,",
    "command": "outline-map.workspace.goToLocation",
    "when": "editorTextFocus"
  },
  // git
  {
    "key": "alt+h",
    "command": "-git.viewFileHistory"
  },
  {
    "key": "shift+alt+g",
    "command": "git.viewFileHistory"
  },
  {
    "key": "ctrlcmd+alt+t",
    "command": "timeline.focus"
  },
  // basic
  {
    "command": "-core.nextTab",
    "keybinding": "ctrlcmd+alt+d"
  },
  {
    "command": "debug.configurations.add",
    "keybinding": "ctrlcmd+alt+d"
  }
]
